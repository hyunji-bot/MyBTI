<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rounded_edge_all"
    android:backgroundTint="#fff"
    android:divider="@color/color3"
    android:dividerPadding="10dp"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:text="INTJ" />

            <EditText
                android:id="@+id/resultMultiLine1_1"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1"
                android:background="@drawable/button_10dp"
                android:backgroundTint="#F4F7FE"
                android:ems="10"
                android:gravity="start|top"
                android:hint="INTJ 설명 - 용의주도한 전략가"
                android:inputType="textMultiLine"
                android:paddingHorizontal="10dp"
                android:paddingVertical="10dp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="20dp"
            android:background="#F4F7FE" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_weight="0"
                android:text="INTP" />

            <EditText
                android:id="@+id/resultMultiLine1_2"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@drawable/button_10dp"
                android:backgroundTint="#F4F7FE"
                android:ems="10"
                android:gravity="start|top"
                android:hint="INTP 설명 - 논리적인 사색가"
                android:inputType="textMultiLine"
                android:paddingHorizontal="10dp"
                android:paddingVertical="10dp" />
        </LinearLayout>

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="20dp"
            android:background="#F4F7FE" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_weight="0"
                android:text="ENTJ" />

            <EditText
                android:id="@+id/resultMultiLine1_3"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@drawable/button_10dp"
                android:backgroundTint="#F4F7FE"
                android:ems="10"
                android:gravity="start|top"
                android:hint="ENTJ 설명 - 대담한 통솔자"
                android:inputType="textMultiLine"
                android:paddingHorizontal="10dp"
                android:paddingVertical="10dp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="20dp"
            android:background="#F4F7FE" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView1_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_weight="0"
                android:text="ENTP" />

            <EditText
                android:id="@+id/resultMultiLine1_4"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:background="@drawable/button_10dp"
                android:backgroundTint="#F4F7FE"
                android:ems="10"
                android:gravity="start|top"
                android:hint="ENTP 설명 - 뜨거운 논쟁을 즐기는 변론가"
                android:inputType="textMultiLine"
                android:paddingHorizontal="10dp"
                android:paddingVertical="10dp" />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="20dp"
                android:background="#F4F7FE" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView1_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="0"
                    android:text="INFJ" />

                <EditText
                    android:id="@+id/resultMultiLine1_5"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="INFJ 설명 - 선의의 옹호자"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="20dp"
                android:background="#F4F7FE" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView1_6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="0"
                    android:text="INFP" />

                <EditText
                    android:id="@+id/resultMultiLine1_6"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="INFP 설명 - 열정적인 중재자"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="20dp"
                android:background="#F4F7FE" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="20dp"
                android:background="#F4F7FE" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView1_7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="0"
                    android:text="ENFJ" />

                <EditText
                    android:id="@+id/resultMultiLine1_7"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="ENFJ 설명 - 정의로운 사회운동가"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="20dp"
                android:background="#F4F7FE" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView1_8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="0"
                    android:text="ENFP" />

                <EditText
                    android:id="@+id/resultMultiLine1_8"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="ENFP 설명 - 재기발랄한 활동가"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>