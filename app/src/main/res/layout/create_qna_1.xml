<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rounded_edge_all"
    android:backgroundTint="#fff"
    android:divider="@color/color3"
    android:dividerPadding="10dp"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rounded_edge_all"
        android:backgroundTint="#fff"
        android:divider="@color/color3"
        android:dividerPadding="10dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:text="Q 1" />

                <EditText
                    android:id="@+id/editTextTextMultiLine1"
                    android:layout_width="match_parent"
                    android:layout_height="98dp"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="외향/내향형 구분 문제"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="15dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView1_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:text="A1 - E" />

                <EditText
                    android:id="@+id/editTextTextMultiLine1_1"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="외향형 답변"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView1_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="0"
                    android:text="A1 - I" />

                <EditText
                    android:id="@+id/editTextTextMultiLine1_2"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="내향형 답변"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/view4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="20dp"
            android:background="#F4F7FE" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:text="Q 2" />

                <EditText
                    android:id="@+id/editTextTextMultiLine2"
                    android:layout_width="match_parent"
                    android:layout_height="98dp"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="외향/내향형 구분 문제"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="15dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView2_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:text="A2 - E" />

                <EditText
                    android:id="@+id/editTextTextMultiLine2_1"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="외향형 답변"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView2_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="0"
                    android:text="A2 - I" />

                <EditText
                    android:id="@+id/editTextTextMultiLine2_2"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="내향형 답변"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="20dp"
            android:background="#F4F7FE" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:text="Q 3" />

                <EditText
                    android:id="@+id/editTextTextMultiLine3"
                    android:layout_width="match_parent"
                    android:layout_height="98dp"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="외향/내향형 구분 문제"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="15dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView3_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:text="A3 - E" />

                <EditText
                    android:id="@+id/editTextTextMultiLine3_1"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="외향형 답변"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView3_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="0"
                    android:text="A3 - I" />

                <EditText
                    android:id="@+id/editTextTextMultiLine3_2"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_10dp"
                    android:backgroundTint="#F4F7FE"
                    android:ems="10"
                    android:gravity="start|top"
                    android:hint="내향형 답변"
                    android:inputType="textMultiLine"
                    android:paddingHorizontal="10dp"
                    android:paddingVertical="10dp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>